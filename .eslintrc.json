{
    "extends": [
        "./.eslintrc.cc.json",
        "plugin:@typescript-eslint/eslint-recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "node": true,
        "commonjs": true,
        "jest": true
    },
    "rules": { 
        "prefer-rest-params": "off"
    }
}
